cAmount\" condition=\"showSocial == true\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"range\" inline title=\"Social Icons Size\" name=\"socialSize\" min=\"1\" max=\"5\" step=\"0.1\" value=\"1.3\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social 1 Color\" value=\"#585858\" name=\"sc1\" condition=\"showSocial == true &amp;&amp; socAmount > 0\">\n        <input type=\"color\" title=\"Social 2 Color\" value=\"#585858\" name=\"sc2\" condition=\"showSocial == true &amp;&amp; socAmount > 1\">\n        <input type=\"color\" title=\"Social 3 Color\" value=\"#585858\" name=\"sc3\" condition=\"showSocial == true &amp;&amp; socAmount > 2\">\n        <input type=\"color\" title=\"Social 4 Color\" value=\"#206489\" name=\"sc4\" condition=\"showSocial == true &amp;&amp; socAmount > 3\">\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#f1f4fa\" selected>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Background Gradient\" name=\"gradient\" condition=\"bg.type == 'color'\">\n        <input type=\"color\" title=\"Color 2\" name=\"bgcolor2\" value=\"#ffffff\" condition=\"gradient == true && bg.type == 'color'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"checkbox\" title=\"Overlay Gradient\" name=\"overlayGradient\" condition=\"overlay == true && bg.type == 'image'\" checked>\n        <input type=\"color\" title=\"Overlay Color 2\" name=\"ovcolor2\" value=\"#022e93\" condition=\"overlay == true && overlayGradient == true && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container\">\n        <h1 class=\"mbr-fonts-style mbr-section-title3\" mbr-theme-style=\"display-2\" mbr-if=\"showmainTitle\" data-app-selector=\".mbr-section-title3\">\n            Launch Your Startup</h1>\n            <h2 class=\"mbr-fonts-style subtitle mbr-text\" mbr-theme-style=\"display-5\" data-app-selector=\".subtitle, .mbr-section-btn\" mbr-if=\"showsubTitle\">Sed malesuada tempus magna, id bibendum lacus interdum ac</h2>\n            <div class=\"mbr-row mbr-justify-content-center\">\n                <div class=\"card mbr-col-sm-12 mbr-col-md-12\" mbr-class=\"{\n                'mbr-col-lg-6': cardsAmount == 1,\n                'mbr-col-lg-4': cardsAmount == 2,\n                'last-child': cardsAmount == 1}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"ico-wrap\" mbr-if=\"showIco\">\n                        <span class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont ico-color mbrib-help\"></span>\n                        </span>\n                        <svg class=\"icon-gradient\">\n                            <defs>\n                                <linearGradient id=\"icoGradient\" x1=\"0%\" y1=\"0%\" x2=\"0%\" y2=\"100%\">\n                                    <stop offset=\"0%\" stop-color=\"{{iconsColor1}}\"></stop>\n                                    <stop offset=\"100%\" stop-color=\"{{iconsColor2}}\"></stop>\n                                </linearGradient>\n                            </defs>\n                        </svg>\n                